{
  "name": "gohighlevel-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for GoHighLevel API integration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:optimized": "bash scripts/dev-optimized.sh",
    "dev:fast": "vite --force",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "analyze": "cross-env ANALYZE=true npm run build",
    "build:analyze": "npm run analyze",
    "env:validate": "node -e \"console.log('Environment validation:', process.env.VITE_SUPABASE_URL ? '✅ Valid' : '❌ Missing VITE_SUPABASE_URL')\"",
    "env:check": "node -e \"console.log('Supabase URL:', process.env.VITE_SUPABASE_URL || 'Not set'); console.log('Anon Key:', process.env.VITE_SUPABASE_ANON_KEY ? 'Set' : 'Not set')\""
  },
  "keywords": [
    "mcp",
    "gohighlevel",
    "api",
    "crm",
    "marketing"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@playwright/test": "^1.55.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@sentry/react": "^10.17.0",
    "@supabase/supabase-js": "^2.58.0",
    "@tanstack/react-query": "^5.90.2",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^5.0.4",
    "axios": "^1.12.2",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.544.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.3",
    "recharts": "^3.2.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.18",
    "terser": "^5.44.0",
    "vite": "^7.1.9",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@types/express": "^5.0.3",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.37.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-react-refresh": "^0.4.23",
    "jsdom": "^24.0.0",
    "lint-staged": "^16.2.3",
    "rollup-plugin-visualizer": "^6.0.4",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
