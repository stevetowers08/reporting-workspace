{
  "functions": {
    "api/leadconnector/oath.mjs": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/((?!api/|logos/|favicon|assets/).*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
