# Test Environment Configuration
# Copy this file to .env.test and update with your test values

# Supabase Configuration (Test Database)
VITE_SUPABASE_URL=https://test-project.supabase.co
VITE_SUPABASE_ANON_KEY=test-anon-key-here

# API Configuration
VITE_API_BASE_URL=http://localhost:3001
VITE_API_TIMEOUT=5000

# Facebook Ads API (Test)
VITE_FACEBOOK_APP_ID=test-facebook-app-id
VITE_FACEBOOK_APP_SECRET=test-facebook-app-secret
VITE_FACEBOOK_REDIRECT_URI=http://localhost:5173/auth/facebook/callback

# Google Ads API (Test)
VITE_GOOGLE_ADS_CLIENT_ID=test-google-client-id
VITE_GOOGLE_ADS_CLIENT_SECRET=test-google-client-secret
VITE_GOOGLE_ADS_REDIRECT_URI=http://localhost:5173/auth/google/callback
VITE_GOOGLE_ADS_DEVELOPER_TOKEN=test-developer-token

# Go High Level API (Test)
VITE_GHL_API_KEY=test-ghl-api-key
VITE_GHL_LOCATION_ID=test-ghl-location-id

# Google Sheets API (Test)
VITE_GOOGLE_SHEETS_CLIENT_ID=test-sheets-client-id
VITE_GOOGLE_SHEETS_CLIENT_SECRET=test-sheets-client-secret
VITE_GOOGLE_SHEETS_REDIRECT_URI=http://localhost:5173/auth/sheets/callback

# Application Configuration
VITE_APP_NAME=Marketing Analytics Dashboard (Test)
VITE_APP_VERSION=1.0.0-test
VITE_APP_ENV=test
VITE_APP_URL=http://localhost:5173

# Security (Test - Use different keys!)
VITE_JWT_SECRET=test-jwt-secret
VITE_ENCRYPTION_KEY=test-encryption-key

# Monitoring & Analytics (Test - Disabled)
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=

# Feature Flags (Test)
VITE_ENABLE_DEBUG=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_PERFORMANCE_MONITORING=false

# Rate Limiting (Test - Very lenient)
VITE_API_RATE_LIMIT=100000
VITE_API_RATE_WINDOW=3600000

# Cache Configuration (Test - Very short TTL)
VITE_CACHE_TTL=1000
VITE_CACHE_MAX_SIZE=10

# Logging (Test - Minimal)
VITE_LOG_LEVEL=error
VITE_LOG_ENABLE_CONSOLE=false
VITE_LOG_ENABLE_REMOTE=false
