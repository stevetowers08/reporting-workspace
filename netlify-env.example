# Netlify Environment Variables Template
# Copy this to .env.netlify and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# App Configuration
VITE_APP_NAME=Marketing Analytics Dashboard
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=production
VITE_APP_URL=https://your-netlify-site.netlify.app

# Feature Flags
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Performance Settings
VITE_CACHE_TTL=300000
VITE_CACHE_MAX_SIZE=100
VITE_API_TIMEOUT=30000

# Security
VITE_ENCRYPTION_KEY=your_production_encryption_key_here

# Facebook Ads API (if using)
VITE_FACEBOOK_APP_ID=your_facebook_app_id
VITE_FACEBOOK_APP_SECRET=your_facebook_app_secret

# Google Ads API (if using)
VITE_GOOGLE_ADS_CLIENT_ID=your_google_ads_client_id
VITE_GOOGLE_ADS_CLIENT_SECRET=your_google_ads_client_secret

# GoHighLevel API (if using)
VITE_GHL_CLIENT_ID=your_ghl_client_id
VITE_GHL_CLIENT_SECRET=your_ghl_client_secret
